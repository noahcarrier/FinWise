generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int      @id @default(autoincrement())
  username   String
  email      String
  password   String
  salt       String
  created_at DateTime @default(now()) @db.Timestamp(6)
}
